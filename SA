local Tab = Window:Tab({
    Title = "Main",
    Icon = "rbxassetid://15567843390",
    Locked = false,
})

local Section = Tab:Section({ 
    Title = "Farming",
    TextXAlignment = "Left",
    TextSize = 17, -- Default Size
})

local collecting = false
local collectRadius = 5000

-- player, root, character setup
local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local root = character:WaitForChild("HumanoidRootPart")

player.CharacterAdded:Connect(function(char)
    character = char
    root = character:WaitForChild("HumanoidRootPart")
end)

-- fun√ß√£o de coleta
local function collectNearbyItems()
    if not root or not root.Parent then return end
    for _, obj in pairs(workspace:GetDescendants()) do
        if obj:IsA("BasePart") and (obj.Position - root.Position).Magnitude <= collectRadius then
            for _, child in ipairs(obj:GetChildren()) do
                if child:IsA("TouchTransmitter") then
                    pcall(function()
                        firetouchinterest(root, obj, 0)
                        firetouchinterest(root, obj, 1)
                    end)
                end
            end
        end
    end
end

-- loop da coleta
task.spawn(function()
    while true do
        if collecting then
            pcall(collectNearbyItems)
        end
        task.wait(0.3)
    end
end)

-- toggle UI
local Toggle = Tab:Toggle({
    Title = "Auto get Item",
    Desc = "Pick up all the items on the floor",
    Icon = "circle",
    Type = "Checkbox",
    Default = false,
    Callback = function(state) 
        collecting = state
        print("Coleta autom√°tica: " .. tostring(state))
    end
})

-- üß† Estado do toggle
local usandoBanknote = false
local player = game:GetService("Players").LocalPlayer

-- üè¶ Fun√ß√£o que usa o Banknote
local function usarBanknote()
    for _, item in pairs(player.Backpack:GetChildren()) do
        if item.Name == "Banknote" then
            item.Parent = player.Character
            task.wait(0.1)
            item:Activate()
            print("Banknote utilizado.")
            return
        end
    end
end

-- üîÅ Loop cont√≠nuo
task.spawn(function()
    while true do
        if usandoBanknote then
            pcall(usarBanknote)
        end
        task.wait(0.5) -- pode diminuir se quiser mais r√°pido
    end
end)

local Toggle = Tab:Toggle({
    Title = "Auto use Banknote",
    Desc = "Use all Banknotes in your inventory",
    Icon = "circle",
    Type = "Checkbox",
    Default = false,
    Callback = function(state) 
        usandoBanknote = state
        print("Auto Banknote: " .. tostring(state))
    end
})

