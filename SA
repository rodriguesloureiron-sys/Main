local isAllowed = allowedUsers[player.UserId] or false

-- D4C SPAM CLONE
local ButtonD4C = Tab:Button({
    Title = "D4C Spam Clone",
    Desc = "Made By Snow",
    Locked = not isAllowed, -- bloqueado se n√£o for permitido
    Callback = function()
        local rep = game:GetService("ReplicatedStorage")
        local plr = game.Players.LocalPlayer

        local function spawnClones(qtd)
            for i = 1, qtd do
                rep.Main.Input:FireServer("Alternate", "Clone")
                wait(0.1)
            end
        end

        while true do
            spawnClones(5)
            wait(0.1)
        end

        print("clicked")
    end
})

-- Kill Aura Anubis
local ButtonKillAura = Tab:Button({
    Title = "kill aura anubis",
    Desc = "Made by unknown",
    Locked = not isAllowed,
    Callback = function()
        local function GetNearestPlr(range)
            local players = game:GetService("Players"):GetPlayers()
            local localPlayer = game:GetService("Players").LocalPlayer
            local localChar = localPlayer.Character
            if not localChar then return nil end
            local localRoot = localChar:FindFirstChild("HumanoidRootPart")
            if not localRoot then return nil end

            local nearestPlayer = nil
            local nearestDistance = math.huge

            for _, player in ipairs(players) do
                if player ~= localPlayer then
                    local char = player.Character
                    if char then
                        local humanoid = char:FindFirstChild("Humanoid")
                        local rootPart = char:FindFirstChild("HumanoidRootPart")
                        if humanoid and humanoid.Health > 0 and rootPart then
                            local distance = (localRoot.Position - rootPart.Position).Magnitude
                            if distance <= range and distance < nearestDistance then
                                nearestPlayer = player
                                nearestDistance = distance
                            end
                        end
                    end
                end
            end

            return nearestPlayer
        end

        while wait() do
            local targetplayer = GetNearestPlr(30)
            if targetplayer then
                pcall(function()
                    local args = {
                        [1] = "Damage",
                        [2] = "StrongSlash",
                        [4] = true,
                        [5] = targetplayer.Character.Humanoid
                    }
                    game:GetService("ReplicatedStorage").Main.Input:FireServer(unpack(args))
                end)
            end
        end

        print("clicked")
    end
})

-- Ice STW
local ButtonIceSTW = Tab:Button({
    Title = "Ice Punch (STW)",
    Desc = "Made By Arc's Hub",
    Locked = not isAllowed,
    Callback = function()
        local Players = game:GetService("Players")
        local ReplicatedStorage = game:GetService("ReplicatedStorage")
        local player = Players.LocalPlayer

        if game:GetService("CoreGui"):FindFirstChild("DamageButtonGui") then
            game:GetService("CoreGui").DamageButtonGui:Destroy()
        end

        local screenGui = Instance.new("ScreenGui")
        screenGui.Name = "DamageButtonGui"
        screenGui.Parent = game:GetService("CoreGui")

        local button = Instance.new("TextButton")
        button.Name = "DamageButton"
        button.Parent = screenGui
        button.Text = "Dano"
        button.Font = Enum.Font.SourceSansBold
        button.TextColor3 = Color3.new(1, 1, 1)
        button.TextScaled = true
        button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        button.AnchorPoint = Vector2.new(0.5, 0.5)
        button.Position = UDim2.new(0.9, 0, 0.9, 0)
        button.Size = UDim2.new(0, 80, 0, 80)
        button.AutoButtonColor = true
        button.BackgroundTransparency = 0
        button.ClipsDescendants = true
        button.BorderSizePixel = 0
        button.TextWrapped = true

        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(1, 0)
        corner.Parent = button

        button.MouseButton1Click:Connect(function()
            for i, v in pairs(Players:GetPlayers()) do
                if v ~= player and v.Character and v.Character:FindFirstChild("Humanoid") then
                    local args1 = {
                        [1] = "Damage",
                        [2] = "Freeze",
                        [4] = true,
                        [5] = v.Character.Humanoid
                    }
                    ReplicatedStorage.Main.Input:FireServer(unpack(args1))

                    local args2 = {
                        [1] = "Damage",
                        [2] = "BloodSuck",
                        [4] = true,
                        [5] = v.Character.Humanoid
                    }
                    ReplicatedStorage.Main.Input:FireServer(unpack(args2))
                end
            end
        end)

        print("clicked")
    end
})

