local Button = Tab:Button({
    Title = "Inf Knife (STW)",
    Locked = false,
    Callback = function()
        local ScreenGui = Instance.new("ScreenGui")
        ScreenGui.Name = "FacasControlGui"
        ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

        local botao = Instance.new("TextButton")
        botao.Size = UDim2.new(0, 150, 0, 40)
        botao.Position = UDim2.new(0, 10, 0, 10)
        botao.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        botao.TextColor3 = Color3.new(1, 1, 1)
        botao.Font = Enum.Font.SourceSansBold
        botao.TextSize = 20
        botao.Text = "Start Knifes"
        botao.Parent = ScreenGui

        _G.LancarFacas = false

        local function iniciarFacas()
            spawn(function()
                while _G.LancarFacas do
                    local args1 = {"Alternate", "Knife"}
                    game.ReplicatedStorage.Main.Input:FireServer(unpack(args1))

                    local args2 = {"Alternate", "ResetKnife"}
                    game.ReplicatedStorage.Main.Input:FireServer(unpack(args2))

                    wait(0.1)
                end
            end)
        end

        botao.MouseButton1Click:Connect(function()
            _G.LancarFacas = not _G.LancarFacas
            botao.Text = _G.LancarFacas and "Stop Knifes" or "Start Knifes"
            if _G.LancarFacas then iniciarFacas() end
        end)
    end
})

