    TextSize = 17, -- Default Size
})


local Button = Tab:Button({
    Title = "SafePlace",
    Desc = "Teleport To Safeplace",
    Locked = false,
    Callback = function()
local Players = game:GetService("Players")
local player = Players.LocalPlayer

local function createSafePlot()
	if workspace:FindFirstChild("SafeBox_" .. player.UserId) then return end

	local safeY = 10000
	local center = Vector3.new(0, safeY, 0)

	local model = Instance.new("Model")
	model.Name = "SafeBox_" .. player.UserId
	model.Parent = workspace

	local function createPart(name, size, offset, rotation)
		local part = Instance.new("Part")
		part.Name = name
		part.Anchored = true
		part.CanCollide = true
		part.Transparency = 0.5
		part.Size = size
		part.Position = center + offset
		if rotation then
			part.Orientation = rotation
		end
		part.Parent = model
	end

	createPart("Base", Vector3.new(21, 2, 21), Vector3.new(0, 0, 0))
	createPart("WallFront", Vector3.new(20, 13, 2), Vector3.new(0, 6.5, 10))    -- Frente
	createPart("WallLeft", Vector3.new(2, 13, 21), Vector3.new(-10, 6.5, 0))    -- Esquerda
	createPart("WallBack", Vector3.new(20, 13, 2), Vector3.new(0, 6.5, -10))    -- Tr√°s
	-- Sem parede direita / meio
	createPart("Roof", Vector3.new(24, 2, 24), Vector3.new(0, 13, 0))
end

local function teleportPlayer()
	local character = player.Character or player.CharacterAdded:Wait()
	local hrp = character:WaitForChild("HumanoidRootPart")

	repeat task.wait() until workspace:FindFirstChild("SafeBox_" .. player.UserId)

	local pos = Vector3.new(0, 10005, 0)
	hrp.CFrame = CFrame.new(pos)
end

createSafePlot()
task.wait(1)
teleportPlayer()
        print("clicked")
    end
})

