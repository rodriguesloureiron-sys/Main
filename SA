local Tab = Window:Tab({
    Title = "Shop",
    Icon = "rbxassetid://9405933217",
    Locked = false,
})

local running = false

local Toggle = Tab:Toggle({
    Title = "Anti-Equip",
    Desc = "",
    Icon = "shield",
    Default = false,
    Callback = function(state)
        running = state

        -- Inicia loop quando ativado
        if running then
            task.spawn(function()
                while running do
                    local player = game.Players.LocalPlayer
                    local char = player.Character
                    local backpack = player:FindFirstChild("Backpack")

                    if char and backpack then
                        for _, item in ipairs(char:GetChildren()) do
                            if item:IsA("Tool") then
                                item.Parent = backpack
                                print("Desequipado:", item.Name)
                            end
                        end
                    end

                    task.wait(0.1)
                end
            end)
        end

        print("Toggle est√° " .. (running and "ativado" or "desativado"))
    end
})

