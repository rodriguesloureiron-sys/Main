local Button = Tab:Button({
    Title = "Hitbox Reaper",
    Locked = false,
    Callback = function()
        pcall(function()
            local _1 = game
            local _2 = _1:GetService("Players")
            local _3 = _1:GetService("Workspace")
            local _4 = _2.LocalPlayer
            local _5 = _3:FindFirstChild(_4.Name)
            if _5 and _5:FindFirstChild("Stand") and _5.Stand:FindFirstChild("Hitbox") then
                _5.Stand.Hitbox.Size = Vector3.new(50, 7.05, 2.83)
            else
                warn("N√£o encontrou " .. _4.Name .. ".Stand.Hitbox")
            end
        end)
    end
})

local Button = Tab:Button({
    Title = "D4C Bomb Clone",
    Locked = false,
    Callback = function()
        local player = game.Players.LocalPlayer
        local playerGui = player:WaitForChild("PlayerGui")

        local screenGui = Instance.new("ScreenGui")
        screenGui.Name = "D4CBombCloneGui"
        screenGui.ResetOnSpawn = false
        screenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
        screenGui.Parent = playerGui

        local button = Instance.new("TextButton")
        button.Name = "d4c_bomb_clone"
        button.Text = "üí£"
        button.Size = UDim2.new(0, 60, 0, 60)
        button.Position = UDim2.new(1, -80, 1, -220)
        button.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
        button.TextColor3 = Color3.fromRGB(255, 255, 255)
        button.Font = Enum.Font.GothamBold
        button.TextSize = 30
        button.BorderSizePixel = 0
        button.AutoButtonColor = true
        button.Parent = screenGui

        local corner = Instance.new("UICorner")
        corner.CornerRadius = UDim.new(1, 0)
        corner.Parent = button

        button.Activated:Connect(function()
            local character = player.Character
            if not character then return end
            local hrp = character:FindFirstChild("HumanoidRootPart")
            if not hrp then return end

            local nearestPlayer
            local shortestDistance = math.huge

            for _, otherPlayer in ipairs(game.Players:GetPlayers()) do
                if otherPlayer ~= player and otherPlayer.Character then
                    local otherHrp = otherPlayer.Character:FindFirstChild("HumanoidRootPart")
                    if otherHrp then
                        local dist = (hrp.Position - otherHrp.Position).Magnitude
                        if dist < shortestDistance then
                            shortestDistance = dist
                            nearestPlayer = otherPlayer
                        end
                    end
                end
            end

            if nearestPlayer and nearestPlayer.Character then
                local leftArm = nearestPlayer.Character:FindFirstChild("Left Arm")
                if leftArm then
                    game.ReplicatedStorage.Main.Input:FireServer("Alternate", "CloneBomb", leftArm)
                    print("D4C Bomb Clone used on:", nearestPlayer.Name)
                else
                    warn("Target missing Left Arm!")
                end
            else
                warn("No nearby players found!")
            end
        end)
    end
})

